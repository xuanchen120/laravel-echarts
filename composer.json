{
  "name": "xuanchen/laravel-echarts",
  "description": "Build ECharts using laravel",
  "keywords": [
    "laravel",
    "charts",
    "echarts"
  ],
  "homepage": "https://github.com/xuanchen120/laravel-echrts.git",
  "license": "MIT",
  "authors": [
    {
      "name": "玄尘",
      "email": "122383162@qq.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "laravel/framework": "*",
    "illuminate/support": "^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "XuanChen\\LaravelECharts\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "XuanChen\\LaravelECharts\\LaravelEChartsServiceProvider"
      ],
      "aliases": {
        "Chart": "XuanChen\\LaravelECharts\\Facade"
      }
    }
  }
}
